{
  "name": "koa-locales",
  "version": "1.12.0",
  "description": "koa locales, i18n solution for koa",
  "main": "index.js",
  "files": [
    "index.js"
  ], 
  "repository": {
    "type": "git",
    "url": "git://github.com/koajs/locales.git",
    "web": "https://github.com/koajs/locales"
  }, 
  "scripts": {
    "lint": "xo",
    "pretest": "npm run lint",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "ci": "npm run coverage"
  },
  "keywords": [
    "koa-locales",
    "i18n",
    "locales",
    "koa-i18n",
    "koa"
  ],
  "author":  "fengmk2 <fengmk2@gmail.com> (https://fengmk2.com)",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.0",
    "humanize-ms": "^1.2.0",
    "ini": "^1.3.4",
    "js-yaml": "^3.13.1",
    "object-assign": "^4.1.0"
  },
  "devDependencies": {
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.3",
    "contributors": "*",
    "eslint-config-xo-lass": "^1.0.3",
    "koa": "^2.12.0",
    "koa-convert": "^1.2.0",
    "mm": "^2.0.0",
    "mocha": "4",
    "nyc": "^15.1.0",
    "pedding": "^1.1.0",
    "supertest": "^2.0.1",
    "xo": "^0.25.3"
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "extends": [
      "xo-lass"
    ],
    "ignore": [
      "test",
      "benchmark"
    ],
    "rules": {
      "node/no-deprecated-api": "off",
      "no-unused-vars": "off",
      "no-prototype-builtins": "off",
      "prefer-rest-params": "off"
    }
  },
  "engines": {
    "node": ">= 8"
  },
  "homepage": "https://github.com/koajs/locales",
  "bugs": {
    "url": "https://github.com/koajs/locales/issues"
  }
}